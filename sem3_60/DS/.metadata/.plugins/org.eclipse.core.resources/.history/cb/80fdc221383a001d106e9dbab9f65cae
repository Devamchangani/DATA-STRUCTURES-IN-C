#include<stdio.h>
#include<stdlib.h>

struct node{
	int data;
	struct node *next;
}*first,*last,*nn,*temp,*curr,*prev,*tt;

int size=0,len=0;

void insertion()
{
	if(len>=size)
	{
		printf("\n > Queue will be overflow !!! ");
		return;
	}
	else
	{
		nn = (struct node *) malloc (sizeof(nn));
		printf("\n > Enter the value : ");
		scanf("%d",&nn->data);
		nn->next=NULL;

		if(first==NULL)
		{
			first=nn;
			last=nn;
		}
		else
		{
			last->next=nn;
			last=nn;
		}
		len++;
	}
}

